      "required": true,
      "enum": [
        "Life Insurance",
        "Investment",
        "Policy Guide",
        "Term Insurance"
      ]
    },
    "imgsrc": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "content": {
      "type": "text",
      "required": true
    }
  }
}
